case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../extract-zip/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../extract-zip/cli.js" "$@"
  ret=$?
fi
exit $ret
